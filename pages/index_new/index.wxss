/**index.wxss**/


.row {

}
.msg_row {
  display: flex;
  flex-direction: row;

  padding: 10rpx;
}
.msg_icon {
 justify-content: flex-end;
 display: flex;
  
background-image: url(data:image/png;base64,);
}

.msg_num {
  display: flex;
}




.main-body-content {
  
  width: 100%;
  padding: 0 0 70rpx;


}

.home-page-content {
  width: 100%;
  padding: 0 0 65rpx;
}
.home-header-content {
  width: 100%;
  height: 300rpx;
  margin: 0;
  background-image: url("https://sdxyt.sdu.edu.cn/tp_jp/resource/image/mobile/home/bg/bg-01.png");
  background-size: 100%;
  background-repeat: no-repeat;
}

.header-option {
  width: 100%;
  margin: 10rpx;
}
.right-btn-box {
  float: right;
}
.mag-btn {
  position: relative;
float: left;
width: 28rpx;
height: 38rpx;
padding: 10rpx 5rpx;
background-size: auto 18rpx;
background-repeat: no-repeat;
background-position: center;
background-image: url(data:image/png;base64,);
}
.home-header-content .right-btn-box .mag-btn .num-box {
  position: absolute;
  top: 5px;
  right: 5px;
  display: block;
  min-width: 16px;
  height: 16px;
  font-size: 12px;
  line-height: 16px;
  text-align: center;
  background-color: #fff;
  border-radius: 8px;
}
.color-theme-100 {
  color: #a50001 !important;
}
.cl-after {

}


.top {
  width: 100%;
  padding: 0 0 65px;
  /* width: calc(100% + 20px); */
  margin: 0 -10px;
  padding: 15px 15px 0;
  background-position: center -20px;
  background-image: url("https://sdxyt.sdu.edu.cn/tp_jp/resource/image/mobile/home/bg/bg-01.png");
  background-size: 100%;
  background-repeat: no-repeat;
}


.time-box {
  float: left;
  margin: 0 10px 0 0;
  font-size: 15px;
  line-height: 18px;
  color: #fff;
}